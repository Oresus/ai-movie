import chunker


def who(string):
    if "directed" in string:
        print 'director'
    elif "made" in string:
        print 'director'
    elif "starred" in string:
        print 'actor'
    elif "was in" in string:
        print 'actor'

input = raw_input('Please enter a question: ')

